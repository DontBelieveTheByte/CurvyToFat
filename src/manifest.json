{
	"manifest_version": 2,
	"name": "Curvy to fat",
	"version": "1.3",
	"description": "Replaces the text 'curvy' with 'fat' and 'fat' with 'honest'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
  "icons": {
			 "16": "icon-16.png",
			 "48": "icon-48.png",
			 "128": "icon-128.png"
  }
}

